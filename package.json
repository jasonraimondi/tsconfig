{
  "name": "@jmondi/tsconfig",
  "version": "0.2.0-alpha.3",
  "author": "Jason Raimondi <jason@raimondi.us>",
  "description": "An opinionated TypeScript config preset for strict and standardized development. Configs for both Node.js and browser environments.",
  "exports": {
    ".": {
      "import": "./tsconfig.node.json",
      "require": "./tsconfig.node.json"
    },
    "./node": {
      "import": "./tsconfig.node.json",
      "require": "./tsconfig.node.json"
    },
    "./browser": {
      "import": "./tsconfig.browser.json",
      "require": "./tsconfig.browser.json"
    },
    "./strict": {
      "import": "./tsconfig.strict.json",
      "require": "./tsconfig.strict.json"
    }
  },
  "files": [
    "tsconfig.*.json"
  ],
  "license": "MIT",
  "private": false
}
